{
  "name": "api-work",
  "version": "1.0.0",
  "description": "just api for my manage my web",
  "main": "dist/app.js",
  "scripts": {
    "start": "if-env NODE_ENV=production ?? npm run start:prod || npm run start:dev",
    "start:prod": "npm run prisma  && npm run build && npm run run",
    "start:dev": "npm run  build-css && npm run copy_static_assets && npm-run-all --parallel build:watch run:watch",
    "server:watch": "nodemon --watch './' --exec ts-node app/app.ts",
    "test": "node --max-old-space-size=2048 ./node_modules/jest/bin/jest.js --detectOpenHandles --colors --maxWorkers=2",
    "build": "webpack/bin/webpack.js",
    "build-css": "tailwindcss build view/admin/index.css -o dist/public/css/admin.css",
    "pre:prod": "npm run prisma && npm run db-seed && npm run  build-css && npm run copy_static_assets  && npm run test && node --max-old-space-size=2048 ./node_modules/webpack/bin/webpack.js",
    "build:watch": "webpack --watch",
    "run": "node ./dist/app.js",
    "run:watch": "nodemon ./dist/app.js",
    "copy_static_assets": "npx ts-node ./utils/copyStaticAssets.ts",
    "db-seed": "npx ts-node ./utils/seeder.ts",
    "prisma": "cd ./prisma && prisma deploy",
    "deploy": "./deploy.sh"
  },
